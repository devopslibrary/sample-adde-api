{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": ["name", "lastUpdated", "shortname", "dns", "cidr", "vlans"],
  "properties": {
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The Name Schema",
      "default": "",
      "examples": ["San Francisco Production Datacenter 01"],
      "pattern": "^(.*)$"
    },
    "lastUpdated": {
      "$id": "#/properties/lastUpdated",
      "type": "string",
      "title": "The Lastupdated Schema",
      "default": "",
      "examples": ["2019-12-30T19:31:41Z"],
      "pattern": "^(.*)$"
    },
    "shortname": {
      "$id": "#/properties/shortname",
      "type": "string",
      "title": "The Shortname Schema",
      "default": "",
      "examples": ["sfo01pr"],
      "pattern": "^(.*)$"
    },
    "dns": {
      "$id": "#/properties/dns",
      "type": "object",
      "title": "The Dns Schema",
      "required": ["primary", "secondary"],
      "properties": {
        "primary": {
          "$id": "#/properties/dns/properties/primary",
          "type": "string",
          "title": "The Primary Schema",
          "default": "",
          "examples": ["9.9.9.9"],
          "pattern": "^(.*)$"
        },
        "secondary": {
          "$id": "#/properties/dns/properties/secondary",
          "type": "string",
          "title": "The Secondary Schema",
          "default": "",
          "examples": ["12.12.12.12"],
          "pattern": "^(.*)$"
        }
      }
    },
    "cidr": {
      "$id": "#/properties/cidr",
      "type": "string",
      "title": "The Cidr Schema",
      "default": "",
      "examples": ["172.64.0.0/19"],
      "pattern": "^(.*)$"
    },
    "vlans": {
      "$id": "#/properties/vlans",
      "type": "array",
      "title": "The Vlans Schema",
      "items": {
        "$id": "#/properties/vlans/items",
        "type": "integer",
        "title": "The Items Schema",
        "default": 0,
        "examples": [11, 12, 13, 14, 15]
      }
    }
  }
}
